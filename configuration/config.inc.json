{
  "domains": [
    "humanrightsdeclaration.local",
    "test.humanrightsdeclaration.scot",
    "humanrightsdeclaration.scot",
    "www.humanrightsdeclaration.scot"
  ],
  "routes": [
    "{:./routes/*.json}"
  ],
  "metaData": {
    "assetsVersion": "{:../../../package.json@version}",
    "assetsBase": "https://storage.googleapis.com/scvo-assets/test/humanrightsdeclaration/",
    "menus": "{:./data/menus.json}",
    "handlebars": {
      "partials": {
        "{$./partials/**/*.*}": "includeRelativePath=false&regexExpression=(-)&regexOptions=gi&regexReplace=_"
      },
      "templates": {
        "{$./templates/**/*.*}": ""
      },
      "layouts": {
        "{$./layouts/**/*.*}": ""
      },
      "queries": {
        "{$./queries/**/*.*}": ""
      }
    }
  }
}